<div class="month-selector" id="monthSelectorGroup" data-selected-month="<%= @selected_month %>" >
  <%= link_to cat_prev_month_path, method: :patch, remote: true do %>
    <%= icon('fas', 'chevron-left', id: "prevMonth") %>
  <% end %>
  <h6 id="monthSelector"><%= @selected_month %></h6>
  <%= link_to cat_next_month_path, method: :patch, remote: true do %>
    <%= icon('fas', 'chevron-right', id: "nextMonth") %>
  <% end %>
</div>

