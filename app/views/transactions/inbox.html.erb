<div class="inbox-list container">
  <div class="inbox-list-div list-header">
    <h4>Category</h4>
    <h4>Date</h4>
    <h4>Info</h4>
    <h4>Spent</h4>
    <%= render 'components/month_selector' %>
  </div>
  <div class="inbox-list-elements">
  <% @transactions.each do |trans| %>
    <%= render 'components/inbox', trans: trans, index: trans.id %>
  <% end %>
  </div>
</div>
